{
  "type": "module",
  "scripts": {
    "dev": "echo \"export default false\" > ./src/config/is_prod.ts && vite",
    "client": "vite --mode client",
    "build": "$npm_execpath run version && vite build --mode client && cp -f ./dist/.vite/manifest.json ./src/lib/manifest.json && vite build",
    "preview": "wrangler pages dev dist",
    "deploy": "$npm_execpath run build && wrangler pages deploy dist",
    "version": "echo \"export default `date +'%s'`\" > ./src/config/build_time.ts && echo \"export default true\" > ./src/config/is_prod.ts",
    "diff": "prisma migrate diff --from-local-d1 --to-schema-datamodel ./prisma/schema.prisma --script",
    "postdiff": "sed -i '' -e '/PRAGMA foreign_key_check;/d' migrations/*.sql",
    "migrate": "wrangler d1 migrations apply memsight"
  },
  "dependencies": {
    "@hono/react-renderer": "^0.2.0",
    "@hookform/resolvers": "^3.3.4",
    "@prisma/adapter-d1": "^5.13.0",
    "@prisma/client": "^5.13.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@upstash/vector": "^1.0.7",
    "axios": "^1.6.8",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dayjs": "^1.11.10",
    "hono": "^4.2.5",
    "jotai": "^2.8.0",
    "lucide-react": "^0.378.0",
    "next-themes": "^0.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.51.3",
    "react-infinite-scroller": "^1.2.6",
    "sonner": "^1.4.41",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "usehooks-ts": "^3.1.0",
    "zod": "^3.23.5"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240403.0",
    "@hono/vite-cloudflare-pages": "^0.2.4",
    "@hono/vite-dev-server": "^0.12.0",
    "@types/node": "^20.12.7",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@types/react-infinite-scroller": "^1.2.5",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "prisma": "^5.13.0",
    "rollup-preserve-directives": "^1.1.1",
    "tailwindcss": "^3.4.3",
    "vite": "^5.0.12",
    "wrangler": "^3.47.0"
  }
}
